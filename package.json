{
    "name": "clerk-demo-app",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "typecheck": "tsc --noEmit",
        "lint": "next lint && prettier --check .",
        "lint:fix": "next lint --fix && prettier --write .",
        "lighthouse": "lighthouse --no-enable-error-reporting $([ 'zzz' = \"zzz$(command -v gp)\" ] && echo 'http://localhost:3000' || gp url 3000)",
        "test": "vitest run",
        "test:unit": "vitest run",
        "playwright": "playwright",
        "test:e2e": "playwright test",
        "deploy": "cdk deploy",
        "test:watch": "vitest --watch"
    },
    "dependencies": {
        "@clerk/nextjs": "*",
        "@mantine/core": "*",
        "@mantine/form": "*",
        "@mantine/modals": "*",
        "@mantine/notifications": "*",
        "@tabler/icons-react": "*",
        "aws-cdk-lib": "*",
        "jsonwebtoken": "*",
        "next": "*",
        "qrcode.react": "*",
        "react": "*",
        "react-dom": "*"
    },
    "devDependencies": {
        "@clerk/testing": "*",
        "@clerk/types": "*",
        "@happy-dom/jest-environment": "*",
        "@playwright/test": "*",
        "@testing-library/react": "*",
        "@types/jsonwebtoken": "*",
        "@types/node": "*",
        "@types/react": "*",
        "@types/react-dom": "*",
        "@vitejs/plugin-react": "*",
        "@vitest/coverage-v8": "*",
        "babel-plugin-module-resolver": "*",
        "cdk-nextjs-standalone": "*",
        "dotenv": "*",
        "eslint": "*",
        "eslint-config-next": "*",
        "eslint-config-prettier": "*",
        "jest": "*",
        "lighthouse": "*",
        "next-router-mock": "*",
        "prettier": "*",
        "source-map-support": "*",
        "ts-node": "*",
        "typescript": "*",
        "vike": "*",
        "vite-tsconfig-paths": "*",
        "vitest": "*"
    }
